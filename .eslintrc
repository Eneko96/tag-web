{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
        {
            "env": {
                "node": true
            },
            "files": [
                ".eslintrc.{js,cjs}"
            ],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "react/react-in-jsx-scope": "off",
        "react/no-children-prop": "off",
        "prettier/prettier": "warn",
        "camelcase": "off",
        "react/prop-types": "off",
        "no-unused-vars": "warn",
        "no-absolute-path": "off"
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ]
}